{
  "name": "uce-template",
  "version": "0.0.6",
  "description": "WIP - a Vue 3 inspired concept based on Custom Elements",
  "bin": "./bin/index.js",
  "main": "./cjs/index.js",
  "scripts": {
    "build": "npm run graph && npm run cjs && npm run rollup:es && npm run rollup:babel && npm run min",
    "cjs": "ascjs esm cjs",
    "graph": "cp node_modules/reactive-props/state.js test/js/reactive-props.js",
    "rollup:es": "rollup --config rollup/es.config.js && sed -i.bck 's/^var /self./' es.js && rm -rf es.js.bck",
    "rollup:babel": "rollup --config rollup/babel.config.js && sed -i.bck 's/^var /self./' index.js && rm -rf index.js.bck",
    "min": "terser index.js --comments='/^!/' -c -m -o min.js"
  },
  "keywords": [
    "custom",
    "elements",
    "template",
    "uce"
  ],
  "author": "Andrea Giammarchi",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "ascjs": "^4.0.1",
    "reactive-props": "^0.1.10",
    "rollup": "^2.28.1",
    "rollup-plugin-terser": "^7.0.2"
  },
  "module": "./esm/index.js",
  "type": "module",
  "exports": {
    ".": {
      "import": "./esm/index.js",
      "default": "./cjs/index.js"
    },
    "./package.json": "./package.json"
  },
  "unpkg": "min.js",
  "dependencies": {
    "@ungap/create-content": "^0.2.0",
    "@ungap/custom-elements": "^0.1.4",
    "augmentor": "^2.2.0",
    "csso": "^4.0.3",
    "html-minifier-terser": "^5.1.1",
    "qsa-observer": "^1.1.5",
    "tag-params": "^1.0.6",
    "terser": "^4.8.0",
    "uce": "^1.11.2",
    "uce-require": "0.0.13"
  }
}
